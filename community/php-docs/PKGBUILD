pkgname=php-docs
pkgver=5.3.0
pkgrel=1
pkgdesc="Set of HTML documentation for PHP."
arch=('any')
url="http://www.php.net/docs.php"
license=('PHP')
depends=('php')
options=('docs')
source=(http://www.php.net/distributions/manual/php_manual_en.tar.gz)
md5sums=('2b4af08ba478df7369443e994cbde4c8')

build(){
  cd $startdir
  install -d $pkgdir/usr/share/doc/php/
  cp -rf php-chunked-xhtml/* $pkgdir/usr/share/doc/php/ || return 1
}
