	cd build/nl
	cmake ${srcdir}/kde-l10n-nl-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/nn
	cd build/nn
	cmake ${srcdir}/kde-l10n-nn-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/pa
	cd build/pa
	cmake ${srcdir}/kde-l10n-pa-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/pl
	cd build/pl
	cmake ${srcdir}/kde-l10n-pl-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/pt
	cd build/pt
	cmake ${srcdir}/kde-l10n-pt-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/pt_BR
	cd build/pt_BR
	cmake ${srcdir}/kde-l10n-pt_BR-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/ro
	cd build/ro
	cmake ${srcdir}/kde-l10n-ro-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/ru
	cd build/ru
	cmake ${srcdir}/kde-l10n-ru-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/si
	cd build/si
	cmake ${srcdir}/kde-l10n-si-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/sk
	cd build/sk
	cmake ${srcdir}/kde-l10n-sk-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/sl
	cd build/sl
	cmake ${srcdir}/kde-l10n-sl-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/sr
	cd build/sr
	cmake ${srcdir}/kde-l10n-sr-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/sv
	cd build/sv
	cmake ${srcdir}/kde-l10n-sv-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/tg
	cd build/tg
	cmake ${srcdir}/kde-l10n-tg-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/tr
	cd build/tr
	cmake ${srcdir}/kde-l10n-tr-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/uk
	cd build/uk
	cmake ${srcdir}/kde-l10n-uk-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/wa
	cd build/wa
	cmake ${srcdir}/kde-l10n-wa-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/zh_CN
	cd build/zh_CN
	cmake ${srcdir}/kde-l10n-zh_CN-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

	mkdir -p build/zh_TW
	cd build/zh_TW
	cmake ${srcdir}/kde-l10n-zh_TW-${pkgver} \
		-DCMAKE_BUILD_TYPE=Release \
		-DCMAKE_INSTALL_PREFIX=/usr
	make
	cd ../..

}

